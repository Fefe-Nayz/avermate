{
  "name": "api",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "drizzle-kit": "^0.31.8",
    "turso": "^0.1.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.5",
    "@better-auth/expo": "^1.4.7",
    "@libsql/client": "^0.17.0",
    "@limitable/ratelimit": "^0.0.4",
    "@t3-oss/env-core": "^0.13.8",
    "better-auth": "^1.4.7",
    "discord.js": "^14.25.1",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.8",
    "ky": "^1.14.1",
    "nanoid": "^5.1.6",
    "resend": "^6.6.0",
    "uploadthing": "^7.7.4",
    "zod": "^4.1.13"
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts ",
    "start": "bun run src/index.ts",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio",
    "db:local": "bunx turso dev --db-file dev.db",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  }
